{
  "entrypoint": "index.html",
  "shell": "src/dorel-app.html",
  "fragments": [
    "src/dorel-app-resources.html",
    "src/atoms/dorel-zendesk.html",
    "src/atoms/dorel-hubspot.html",
    "src/organisms/dorel-footer.html",
    "src/templates/dorel-template-info.html",
    "src/templates/dorel-template-products.html"
